{% extends "projects.jinja2" %}
{% block rightbar %}
<div class="col-sm-3">
    <div class="panel panel-default">
      <div class="panel-heading">Specification Sheet</div>
      <div class="panel-body">
        <p><h5>Platform:</h5></p><p>{% block platform %}{% endblock %}</p>
        <p><h5>Languages:</h5></p><p>{% block languages %}{% endblock %}</p>
        <p><h5>Libraries / Frameworks:</h5></p><p>{% block libraries %}{% endblock %}</p>
        {% block extrarightbar %}{% endblock %}
      </div>
    </div>
</div>
{% endblock rightbar %}
